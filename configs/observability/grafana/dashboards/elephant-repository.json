{
  "dashboard": {
    "title": "Elephant Repository Service",
    "tags": ["elephant", "repository", "backend"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
        "type": "stat",
        "title": "Documents Created (1h)",
        "targets": [
          {
            "expr": "sum(increase(documents_created_total[1h]))"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "short"
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
        "type": "stat",
        "title": "Documents Updated (1h)",
        "targets": [
          {
            "expr": "sum(increase(documents_updated_total[1h]))"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
        "type": "stat",
        "title": "Versions Created (1h)",
        "targets": [
          {
            "expr": "sum(increase(document_versions_total[1h]))"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
        "type": "stat",
        "title": "Current Event Log Position",
        "targets": [
          {
            "expr": "event_log_position"
          }
        ],
        "options": {
          "colorMode": "value"
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "type": "graph",
        "title": "Document Operations Rate",
        "targets": [
          {
            "expr": "rate(documents_created_total[5m])",
            "legendFormat": "Created"
          },
          {
            "expr": "rate(documents_updated_total[5m])",
            "legendFormat": "Updated"
          },
          {
            "expr": "rate(documents_deleted_total[5m])",
            "legendFormat": "Deleted"
          },
          {
            "expr": "rate(documents_read_total[5m])",
            "legendFormat": "Read"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Operations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "type": "graph",
        "title": "Documents by Type",
        "targets": [
          {
            "expr": "sum(documents_created_total) by (type)",
            "legendFormat": "{{type}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "type": "graph",
        "title": "S3 Archive Operations",
        "targets": [
          {
            "expr": "rate(s3_upload_total[5m])",
            "legendFormat": "Uploads"
          },
          {
            "expr": "rate(s3_upload_errors_total[5m])",
            "legendFormat": "Errors"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Operations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "type": "graph",
        "title": "S3 Upload Duration",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(s3_upload_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(s3_upload_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P50"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "type": "graph",
        "title": "Event Log Processing",
        "targets": [
          {
            "expr": "rate(event_log_events_written[5m])",
            "legendFormat": "Events written/sec"
          },
          {
            "expr": "event_log_position",
            "legendFormat": "Current position"
          }
        ]
      },
      {
        "id": 10,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "type": "graph",
        "title": "Database Query Duration",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(db_query_duration_seconds_bucket[5m])) by (query, le))",
            "legendFormat": "{{query}} (P95)"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30},
        "type": "graph",
        "title": "ACL Check Performance",
        "targets": [
          {
            "expr": "rate(acl_checks_total[5m])",
            "legendFormat": "Checks/sec"
          },
          {
            "expr": "rate(acl_check_denials_total[5m])",
            "legendFormat": "Denials/sec"
          }
        ]
      },
      {
        "id": 12,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30},
        "type": "graph",
        "title": "Schema Validation",
        "targets": [
          {
            "expr": "rate(schema_validations_total[5m])",
            "legendFormat": "Validations/sec"
          },
          {
            "expr": "rate(schema_validation_failures_total[5m])",
            "legendFormat": "Failures/sec"
          }
        ]
      },
      {
        "id": 13,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 38},
        "type": "table",
        "title": "Top Documents by Version Count",
        "targets": [
          {
            "expr": "topk(20, max(document_version) by (uuid, type))",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "renameByName": {
                "uuid": "Document UUID",
                "type": "Type",
                "Value": "Version Count"
              }
            }
          }
        ]
      }
    ],
    "refresh": "30s",
    "schemaVersion": 27,
    "style": "dark",
    "time": {
      "from": "now-3h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "uid": "elephant-repository",
    "version": 1
  }
}

{
  "dashboard": {
    "title": "Elephant Frontend Performance",
    "tags": ["elephant", "frontend", "faro", "rum"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
        "type": "stat",
        "title": "Active Sessions",
        "targets": [
          {
            "expr": "count(faro_session_info{app=\"elephant-chrome\"})"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
        "type": "stat",
        "title": "Page Views (1h)",
        "targets": [
          {
            "expr": "sum(increase(faro_page_views_total{app=\"elephant-chrome\"}[1h]))"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
        "type": "stat",
        "title": "JavaScript Errors (1h)",
        "targets": [
          {
            "expr": "sum(increase(faro_errors_total{app=\"elephant-chrome\"}[1h]))"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {"value": 0, "color": "green"},
            {"value": 10, "color": "yellow"},
            {"value": 50, "color": "red"}
          ]
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
        "type": "stat",
        "title": "Avg Session Duration",
        "targets": [
          {
            "expr": "avg(faro_session_duration_seconds{app=\"elephant-chrome\"})"
          }
        ],
        "options": {
          "colorMode": "value",
          "unit": "s"
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "type": "graph",
        "title": "Core Web Vitals - LCP (Largest Contentful Paint)",
        "description": "Good: < 2.5s, Needs Improvement: 2.5-4s, Poor: > 4s",
        "targets": [
          {
            "expr": "histogram_quantile(0.75, sum(rate(faro_web_vitals_lcp_bucket{app=\"elephant-chrome\"}[5m])) by (le))",
            "legendFormat": "P75"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(faro_web_vitals_lcp_bucket{app=\"elephant-chrome\"}[5m])) by (le))",
            "legendFormat": "P95"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Time"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 2.5, "colorMode": "critical", "fill": false, "line": true},
          {"value": 4.0, "colorMode": "critical", "fill": false, "line": true}
        ]
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "type": "graph",
        "title": "Core Web Vitals - FCP (First Contentful Paint)",
        "description": "Good: < 1.8s, Needs Improvement: 1.8-3s, Poor: > 3s",
        "targets": [
          {
            "expr": "histogram_quantile(0.75, sum(rate(faro_web_vitals_fcp_bucket{app=\"elephant-chrome\"}[5m])) by (le))",
            "legendFormat": "P75"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(faro_web_vitals_fcp_bucket{app=\"elephant-chrome\"}[5m])) by (le))",
            "legendFormat": "P95"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Time"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 1.8, "colorMode": "critical", "fill": false, "line": true},
          {"value": 3.0, "colorMode": "critical", "fill": false, "line": true}
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "type": "graph",
        "title": "Core Web Vitals - CLS (Cumulative Layout Shift)",
        "description": "Good: < 0.1, Needs Improvement: 0.1-0.25, Poor: > 0.25",
        "targets": [
          {
            "expr": "histogram_quantile(0.75, sum(rate(faro_web_vitals_cls_bucket{app=\"elephant-chrome\"}[5m])) by (le))",
            "legendFormat": "P75"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Score"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 0.1, "colorMode": "critical", "fill": false, "line": true},
          {"value": 0.25, "colorMode": "critical", "fill": false, "line": true}
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "type": "graph",
        "title": "API Call Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(faro_api_call_duration_bucket{app=\"elephant-chrome\"}[5m])) by (endpoint, le))",
            "legendFormat": "{{endpoint}} (P95)"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(faro_api_call_duration_bucket{app=\"elephant-chrome\"}[5m])) by (endpoint, le))",
            "legendFormat": "{{endpoint}} (P50)"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "type": "graph",
        "title": "JavaScript Errors by Type",
        "targets": [
          {
            "expr": "sum(rate(faro_errors_total{app=\"elephant-chrome\"}[5m])) by (error_type)",
            "legendFormat": "{{error_type}}"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "type": "graph",
        "title": "Navigation Timing",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(faro_navigation_timing_bucket{app=\"elephant-chrome\"}[5m])) by (metric, le))",
            "legendFormat": "{{metric}} (P95)"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Time"},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30},
        "type": "graph",
        "title": "Page Views by Route",
        "targets": [
          {
            "expr": "sum(rate(faro_page_views_total{app=\"elephant-chrome\"}[5m])) by (route)",
            "legendFormat": "{{route}}"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Views/sec"},
          {"format": "short"}
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "showLegend": true
          }
        }
      },
      {
        "id": 12,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30},
        "type": "graph",
        "title": "User Interactions",
        "targets": [
          {
            "expr": "sum(rate(faro_user_interactions_total{app=\"elephant-chrome\"}[5m])) by (interaction_type)",
            "legendFormat": "{{interaction_type}}"
          }
        ]
      },
      {
        "id": 13,
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 38},
        "type": "table",
        "title": "Top JavaScript Errors",
        "targets": [
          {
            "expr": "topk(10, sum(increase(faro_errors_total{app=\"elephant-chrome\"}[1h])) by (error_message, error_stack))",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "renameByName": {
                "error_message": "Error Message",
                "error_stack": "Stack Trace",
                "Value": "Count (1h)"
              }
            }
          }
        ]
      },
      {
        "id": 14,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48},
        "type": "piechart",
        "title": "Browser Distribution",
        "targets": [
          {
            "expr": "sum(faro_session_info{app=\"elephant-chrome\"}) by (browser)"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 15,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48},
        "type": "piechart",
        "title": "Device Distribution",
        "targets": [
          {
            "expr": "sum(faro_session_info{app=\"elephant-chrome\"}) by (device_type)"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      }
    ],
    "refresh": "1m",
    "schemaVersion": 27,
    "style": "dark",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "uid": "elephant-frontend",
    "version": 1
  }
}
